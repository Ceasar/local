LEFT=0
TOP=22
RIGHT=1438
BOTTOM=881

CELLS_WIDE=6
CELL_WIDTH=$(( (RIGHT - LEFT) / CELLS_WIDE ))

CELLS_TALL=6
CELL_HEIGHT=$(( (BOTTOM - TOP) / CELLS_TALL ))

NEW_LEFT=$(( (CELL_WIDTH * $1) ))
NEW_TOP=$(( (CELL_HEIGHT * $2) ))
NEW_WIDTH=$(( CELL_WIDTH * $3 ))
NEW_HEIGHT=$(( CELL_HEIGHT * $4 ))

resize $NEW_LEFT $NEW_TOP $NEW_WIDTH $NEW_HEIGHT
